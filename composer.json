{
  "name": "okashoi/php-attributes-example",
  "license": "MIT",
  "require": {
    "php": "~8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Okashoi\\PhpAttributesExample\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Okashoi\\PhpAttributesExample\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --bootstrap=vendor/autoload.php tests"
  }
}
